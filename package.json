{
	"name": "proyek2m",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "npm run generate:importmap && npm run generate:types && cross-env NODE_OPTIONS=--no-deprecation next build --turbopack",
		"dev": "npm run generate:importmap && npm run mantine::generate && cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
		"devsafe": "rm -rf .next && npm run generate:importmap &&  npm run generate:importmap && npm run generate:types && cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
		"mantine::generate": "npx tailwind-preset-mantine ./src/lib/modules/theme-mantine.ts -o ./src/styles/mantine.css",
		"lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"start": "cross-env NODE_OPTIONS=--no-deprecation next start",
		"seed": "npx tsx src/seed.ts"
	},
	"dependencies": {
		"@mantine/carousel": "^8.1.3",
		"@mantine/core": "^8.1.3",
		"@mantine/form": "^8.1.3",
		"@mantine/hooks": "^8.1.3",
		"@mantine/nprogress": "^8.1.3",
		"@payloadcms/db-postgres": "^3.46.0",
		"@payloadcms/email-nodemailer": "^3.46.0",
		"@payloadcms/live-preview-react": "^3.46.0",
		"@payloadcms/next": "^3.46.0",
		"@payloadcms/payload-cloud": "^3.46.0",
		"@payloadcms/plugin-form-builder": "^3.46.0",
		"@payloadcms/plugin-seo": "^3.46.0",
		"@payloadcms/richtext-lexical": "^3.46.0",
		"@payloadcms/storage-s3": "^3.46.0",
		"@payloadcms/ui": "^3.46.0",
		"@vercel/speed-insights": "^1.2.0",
		"@vidstack/react": "^1.12.13",
		"clsx": "^2.1.1",
		"cross-env": "^7.0.3",
		"embla-carousel": "^8.6.0",
		"embla-carousel-class-names": "^8.6.0",
		"embla-carousel-react": "^8.6.0",
		"graphql": "^16.11.0",
		"lucide-react": "^0.525.0",
		"mantine-form-zod-resolver": "^1.3.0",
		"motion": "12.23.3",
		"next": "^15.3.5",
		"payload": "^3.46.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-infinite-scroll-component": "^6.1.0",
		"sharp": "^0.34.3",
		"tailwind-merge": "^3.3.1",
		"tailwind-preset-mantine": "^2.1.0",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "4.1.11",
		"@types/node": "^24.0.13",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.30.1",
		"eslint-config-next": "15.3.5",
		"postcss": "^8.5.6",
		"postcss-preset-mantine": "^1.18.0",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"schema-dts": "^1.1.5",
